 {
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "importOrder": ["^@ui/(.*)$", "^@server/(.*)$", "<THIRD_PARTY_MODULES>", "^[./]"],
    "importOrderSeperation": true,
    "importOrderSortSpecifiers": true,
    "printWidth": 100,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}